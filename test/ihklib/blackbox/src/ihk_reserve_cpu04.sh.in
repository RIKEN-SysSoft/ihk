#!/usr/bin/sh

# define WORKDIR
if [ "${AUTOTEST_HOME}" != "" ]; then
    . ${AUTOTEST_HOME}/bin/config.sh
fi

sudo @CMAKE_INSTALL_PREFIX@/bin/ihk_reserve_cpu04 -u $(id -u) -g $(id -g)
ret=$?

if [ "${AUTOTEST_HOME}" != "" ]; then
    # Pass OK/NG to runtestlist.sh
    echo $ret > $WORKDIR/result.log
fi
