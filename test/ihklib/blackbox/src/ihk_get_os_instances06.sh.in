#!/usr/bin/sh

# define WORKDIR
if [ "${AUTOTEST_HOME}" != "" ]; then
    . ${AUTOTEST_HOME}/bin/config.sh
fi

# insmod as root
sudo @CMAKE_INSTALL_PREFIX@/bin/ihk_get_os_instances06 -u $(id -u) -g $(id -g) -i
num_os_instances=$?

# try to reserve memory as non-root
@CMAKE_INSTALL_PREFIX@/bin/ihk_get_os_instances06 -u $(id -u) -g $(id -g) -n ${num_os_instances}

# rmmod as root
sudo @CMAKE_INSTALL_PREFIX@/bin/ihk_get_os_instances06 -u $(id -u) -g $(id -g) -r
ret=$?


if [ "${AUTOTEST_HOME}" != "" ]; then
    # Pass OK/NG to runtestlist.sh
    echo $ret > $WORKDIR/result.log
fi
